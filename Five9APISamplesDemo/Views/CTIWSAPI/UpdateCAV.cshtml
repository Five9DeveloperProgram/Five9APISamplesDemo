@{
    ViewBag.Title = "Update CAV";
}

<h2>CallEventMonitor</h2>

@section scripts {
    @Scripts.Render("~/Scripts/Five9/xml2json.js")
    @Scripts.Render("~/Scripts/Five9/Flags.js")
    @Scripts.Render("~/Scripts/Five9/Five9CTI-1.0.0.js")
    @Scripts.Render("~/Scripts/Five9/ctiwspage.js")
}


<div class="container">
    
    <div class="col-md-6">
        <div>
            <form role="form">
                <div class="form-group">
                    <label for="CAVName" class="control-label">CAV Field Name to Update</label>
                    <input type="text" class="form-control" id="CAVName" placeholder="Enter CAV Field Name">
                </div>

                <div class="form-group">
                    <label for="CAVValue" class="control-label">CAV Value</label>
                    <input type="text" class="form-control" id="CAVValue" placeholder="Enter CAV Value">
                </div>

                <button type="button" id="updatecav" class="btn btn-default">Update</button>

            </form>
        </div>
        <div class="col-md-6" id="callEventDiv">   </div>
    </div>
</div>